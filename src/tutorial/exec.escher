Exec {
	proc *os.Process
	proc:Command = {
		Path "/bin/ls"
		Args { "/" }
	}
	proc:When = 1

	yio *Fork
	proc:IO = yio:

	yio:Stdin = *Ignore
	yio:Stdout = *os.Stdout
	yio:Stderr = *os.Stderr
	yio:When = *Ignore

	yexit *Fork
	proc:Exit = yexit:
	exit *os.Exit

	yexit:Exit = exit:
	yexit:When = *Ignore
}
