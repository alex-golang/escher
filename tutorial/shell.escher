//
//	Run: escher Shell
//
Shell {
	shell StdShell
	shell:In = escher.Memory
	shell:Out = Ignore
	shell:Name = "tutorial"
}

StdShell { // Name, In, Out
	shell escher.Shell
	shell:In = :In
	shell:Out = :Out
	shell:User = arg:

	arg Fork
	arg:Name = :Name
	arg:In = os.Stdin
	arg:Out = os.Stdout
	arg:Err = os.Stderr
}
