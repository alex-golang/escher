QuickFocus {
	x Focus
	x:View = y:
	x: = :

	y Fork
	y:Root = :Root
	y:Name = :Name
}

QuickAssociate {
	x Associate
	x:View = y:
	x: = :

	y Fork
	y:Root = :Root
	y:Name = :Name
	y:Value = :Value
}

TestAssociate {
	a QuickAssociate
	a:Root = { A "abc" }
	a:Name = "B"
	a:Value = "def"
	a: = match:X
	match:Y = {
		A "abc"
		B "def"
	}
	match testing.Match
	match: = os.Exit
}
