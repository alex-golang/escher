
// Merge ...
Merge {
	fork text.ForkMerge
	fork:X = Merge:X
	fork:Y = Merge:Y
	fork:Z = Merge:Z

	merge text.MergeBlend
	merge:XYZ = fork._
	merge:_ = Merge:_
}

// Form ...
Form {
	fork text.ForkForm
	fork:Form = Form:Form
	fork:Data = Form:Data

	form text.ForkForm
	form:FormData = fork._
	form:_ = Form:_
}
